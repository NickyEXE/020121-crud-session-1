<%= form_for @blog_post do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :author %><br>
  <%= f.text_field :author %><br>
  <%= f.label :outlet %><br>
  <%= f.text_field :outlet %><br>
  <%= f.label :rating %><br>
  <%= f.number_field :rating, in: 1..10 %><br>
  <%= f.label :opinion %><br>
  <%= f.text_area(:opinion, cols: 40, rows: 20) %><br>
  <%= f.submit %>
<% end %>
